{
    "name": "cartalyst/assetic-filters",
    "description": "Additional assetic filters (such as a PHP SASS compiler) to enhance Assetic.",
    "keywords": [
        "php",
        "cartalyst",
        "assetic",
        "filters",
        "assets",
        "themes"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "symfony/http-foundation": "^2.5|^3.0|^4.0",
        "cartalyst/assetic": "^1.2",
        "richthegeek/phpsass": "dev-master"
    },
    "require-dev": {
        "cartalyst/php-cs-fixer-config": "^1.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cartalyst\\AsseticFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cartalyst\\AsseticFilters\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
