{
    "name": "cartalyst/assetic-filters",
    "description": "Additional assetic filters (such as a PHP SASS compiler) to enhance Assetic.",
    "keywords": [
        "php",
        "cartalyst",
        "assetic",
        "filters",
        "assets",
        "themes"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.cartalyst.com" }
    ],
    "require": {
        "php": ">=5.6.4",
        "symfony/http-foundation": "^2.5|^3.0",
        "kriswallsmith/assetic": "^1.2",
        "richthegeek/phpsass": "~1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Cartalyst\\AsseticFilters\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
